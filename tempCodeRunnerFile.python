import numpy as np

# 原始数字
original_numbers = np.array([
    [0.9148, 0.8362, 0.7491]
])

# 生成新的数字
new_numbers = original_numbers + np.random.uniform(low=-0.1, high=0, size=original_numbers.shape)

# 保留小数点后四位
new_numbers = np.round(new_numbers, 4)

# 打印结果
print("原始数字：")
print(original_numbers)
print("\n新生成的数字：")
print(new_numbers)
